<h1>users#index</h1>

<%= render 'users/list', user: @user %>
<%= render 'books/list', book: @book %>

<h1>Users</h1>
<table class="table">
  <tr>
    <td>image</td>
    <td>name</td>
    <td></td>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= attachment_image_tag @user, :profile_image, :fill, 10, 10, fallback: "no_image.jpg" %></th>
    <td><%= user.name %></th>
    <td><%= link_to "Show", user_path(user.id) %></th>
   </tr>
   <% end %>
</table>